{
    "name": "adriandmitroca/wordpress-theme-starter",
    "authors": [
        {
            "name": "Adrian Dmitroca",
            "email": "dmitroca.adrian@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "wordpress-theme",
    "require": {
        "php": "8.3.*",
        "stoutlogic/acf-builder": "1.12.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.8.0",
        "wp-coding-standards/wpcs": "3.0.1"
    },
    "scripts": {
        "lint": [
            "phpcs --standard=.phpcs.xml ./; phpcbf --standard=.phpcs.xml ./"
        ],
        "test": [
            "composer lint"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
