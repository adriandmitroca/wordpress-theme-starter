    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-vue": "10.1.0",
    "eslint-webpack-plugin": "4.2.0",
    "laravel-mix-polyfill": "3.0.1",
    "postcss": "8.5.3",
