    "eslint": "8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "9.1.0",
    "eslint-webpack-plugin": "3.1.1",
    "laravel-mix-polyfill": "3.0.1",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
